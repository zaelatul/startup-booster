{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:5174": "node scripts/predev.mjs && next dev -p 5174",
    "dev:3000": "node scripts/predev.mjs && set PORT=3000 && next dev -p 3000",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "clean:dev": "powershell -NoLogo -NoProfile -ExecutionPolicy Bypass -File ./scripts/clean-dev.ps1",
    "test:e2e": "node scripts/run-e2e.mjs",
    "test:ui": "cross-env PW_NO_SERVER=1 BASE_URL=http://localhost:5174 npx playwright test --ui",
    "report": "npx playwright show-report",
    "check": "npm run typecheck && npm run lint && npm run test:e2e"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@supabase/supabase-js": "^2.80.0",
    "clsx": "^2.1.1",
    "lucide-react": "^0.554.0",
    "next": "^16.0.10",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "recharts": "^3.5.0",
    "swiper": "^12.0.3"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@tailwindcss/postcss": "^4.1.16",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "cross-env": "^10.1.0",
    "eslint": "^9",
    "eslint-config-next": "16.0.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.16",
    "typescript": "^5"
  }
}
